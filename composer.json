{
	"name": "baraja-core/structured-api",
	"description": "Complex library for definition of your structured API endpoint as class with schema.",
	"homepage": "https://github.com/baraja-core/structured-api",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": "^8.0",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-iconv": "*",
		"baraja-core/service-method-invoker": "^2.1",
		"baraja-core/url": "^1.1",
		"nette/di": "^3.0",
		"nette/utils": "^3.2",
		"nette/http": "^3.1",
		"nette/application": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/caching": "^3.1",
		"nette/security": "^3.1"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.74",
		"tracy/tracy": "^2.8",
		"phpstan/phpstan-nette": "^0.12.14",
		"baraja-core/localization": "^2.0",
		"spaze/phpstan-disallowed-calls": "^1.1",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
