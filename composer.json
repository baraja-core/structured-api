{
	"name": "baraja-core/structured-api",
	"description": "Complex library for definition of your structured API endpoint as class with schema.",
	"homepage": "https://github.com/baraja-core/structured-api",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-iconv": "*",
		"baraja-core/service-method-invoker": "1.0",
		"nette/di": "^3.0",
		"nette/utils": "^3.0",
		"nette/http": "^3.0",
		"nette/application": "^3.0",
		"nette/caching": "^3.0",
		"nette/security": "^3.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
